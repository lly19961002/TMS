<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tms.model.ConsigneeInfo" table="consignee_info" schema="tms">
        <id name="id" column="id"/>
        <property name="consignee" column="consignee"/>
        <property name="consigneeContact" column="consignee_contact"/>
        <property name="consigneePhone" column="consignee_phone"/>
        <property name="consigneeAddress" column="consignee_address"/>
    </class>
</hibernate-mapping>